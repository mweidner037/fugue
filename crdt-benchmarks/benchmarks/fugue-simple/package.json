{
  "name": "fugue-simple-bench",
  "version": "1.0.0",
  "description": "Fugue Simple benchmarks",
  "main": "./run.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "([ -f \"./dist/bundle.js\" ] || npm run dist) && node --expose-gc run.js && npm run table",
    "table": "echo 'Fugue Simple results: \n\n' && node ../../bin/render-table.js ../results.json 6000 fugue-simple",
    "dist": "npm run clean && rollup -c && gzip --keep dist/*"
  },
  "author": "Matthew Weidner and Martin Kleppmann",
  "license": "MIT",
  "dependencies": {
    "@collabs/collabs": "0.13.4",
    "seedrandom": "^3.0.5",
    "fugue-simple": "1.0.0"
  }
}
