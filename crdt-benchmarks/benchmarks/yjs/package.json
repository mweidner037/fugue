{
  "name": "yjs",
  "version": "1.0.0",
  "description": "Yjs benchmarks",
  "main": "./run.js",
  "type": "module",
  "scripts": {
    "start": "npm run exp && npm run table",
    "exp": "node --expose-gc run.js",
    "table": "echo 'Yjs results: \n\n' && node ../../bin/render-table.js ../results.json 6000 yjs",
    "B4x100": "node --expose-gc run.js B4x100 && npm run table"
  },
  "author": "Kevin Jahns",
  "license": "MIT",
  "dependencies": {
    "yjs": "13.6.8"
  }
}
