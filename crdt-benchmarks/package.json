{
  "name": "crdt-benchmarks",
  "version": "1.0.0",
  "private": true,
  "description": "CRDT benchmarks, forked from Kevin Jahns's https://github.com/dmonad/crdt-benchmarks",
  "type": "module",
  "workspaces": [
    "benchmarks/yjs",
    "benchmarks/ywasm",
    "benchmarks/automerge-wasm",
    "benchmarks/tree-fugue",
    "benchmarks/tree-fugue-simple",
    "benchmarks/list-fugue-simple"
  ],
  "scripts": {
    "start": "npm run start -ws",
    "table": "node bin/render-table.js benchmarks/results.json 6000 yjs ywasm automerge-wasm tree-fugue tree-fugue-simple list-fugue-simple",
    "lint": "standard && tsc"
  },
  "standard": {
    "ignore": [
      "**/dist/"
    ]
  },
  "engines": {
    "node": "^18.0.0"
  },
  "dependencies": {
    "lib0": "^0.2.42",
    "mathjs": "^11.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "rollup": "^2.77.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard": "^16.0.3",
    "typescript": "^3.9.6"
  }
}
